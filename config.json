{
    "target_urls": [
        "https://example.com"
    ],
    "api_endpoints": [
        "https://example.com/api/v1/users",
        "https://example.com/api/v1/orders"
    ],
    "javascript_security": {
        "scan_for_xss": true,
        "scan_for_insecure_libraries": true,
        "csp_evaluation": true
    },
    "input_validation_checks": {
        "sql_injection": true,
        "xss": true,
        "command_injection": true,
        "deserialization_attack": true
    },
    "api_security": {
        "bola_check": true,
        "rate_limiting": true,
        "auth_validation": true,
        "endpoint_enumeration": true
    },
    "session_management": {
        "secure_cookies": true,
        "session_fixation_check": true,
        "csrf_protection_check": true
    },
    "compliance_mapping": {
        "owasp_top_10": true,
        "cwe_mapping": true
    },
    "alert_thresholds": {
        "critical": 10,
        "high": 7,
        "medium": 5,
        "low": 2
    }
}